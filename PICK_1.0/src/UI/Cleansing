import pandas as pd

########Text reader
orders1 = pd.read_csv(r'C:\Users\jesus\OneDrive\Desktop\data.txt', delimiter = '\t')
#print(orders1)
filtered_data = orders1.dropna(axis='rows', how='all')
filtered_data = orders1.dropna(axis='columns', how='all')
print(filtered_data)

########CSV reader
orders2 = pd.read_csv(r'C:\Users\jesus\OneDrive\Desktop\data.csv')
#print(orders2)
filtered_data = orders2.dropna(axis='rows', how='all')
filtered_data = orders2.dropna(axis='columns', how='all')
print (filtered_data)

########Excel reader
orders3 = pd.read_excel(r'C:\Users\jesus\OneDrive\Desktop\data.xlsx')
#print(orders3)
filtered_data = orders3.dropna(axis='rows', how='all')
filtered_data = orders3.dropna(axis='columns', how='all')
print (filtered_data)


